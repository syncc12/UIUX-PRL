<!DOCTYPE html>
<html>
  <head>
    <title>UIUX</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  </head>

  <body>

    <div id="flex-content-container">
      <nav class="navbar navbar-toggleable-md navbar-inverse bg-inverse">
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbar-content" aria-controls="navbar-content" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <%= link_to root_path do %>
          <%= link_to 'UIUX', root_path, class: 'navbar-brand' %>
        <% end %>
        <div class="collapse navbar-collapse" id="navbar-content">
          <div class="nav-link-custom-div">
            <%= link_to 'Home', root_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Ideas', ideas_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Calculator', calculators_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'RPS', rps_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Piano', pianos_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Thermometer', thermometers_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Checkers', checkers_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Chess', chesses_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Flip A Coin', flip_coins_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Two Up', two_ups_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Wheel', wheels_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Character Counter', character_counters_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'DC Metro', dc_metros_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Minesweeper', minesweepers_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Hexapawn', hexapawns_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Clock', clocks_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Timer', timers_path, class: 'nav-link-custom' %>
          </div>
          <div class="nav-divider"></div>
          <div class="nav-link-custom-div">
            <%= link_to 'Paint', paints_path, class: 'nav-link-custom' %>
          </div>

        </div>

      </nav>
    

      <%= yield %>
  </body>
</html>
