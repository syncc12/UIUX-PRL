  function hasKing(inCoordinate) {
    var c1 = c1c2(inCoordinate)[0],c2 = c1c2(inCoordinate)[1];
    var kingStatus = $('#sq-'+c1+'-'+c2+' div').data('king');
    return kingStatus;
  }

  function possibleMoves(inCoordinate) {
    var c1 = c1c2(inCoordinate)[0],c2 = c1c2(inCoordinate)[1];

    var sSelf = squareStatus(inCoordinate);

    var icTL = num2let(parseInt(let2num(c1))-1)+(parseInt(c2)-1).toString();
    var icTR = num2let(parseInt(let2num(c1))-1)+(parseInt(c2)+1).toString();
    var icBL = num2let(parseInt(let2num(c1))+1)+(parseInt(c2)-1).toString();
    var icBR = num2let(parseInt(let2num(c1))+1)+(parseInt(c2)+1).toString();
    
    var sTL = squareStatus(icTL),sTR = squareStatus(icTR);
    var sBL = squareStatus(icBL),sBR = squareStatus(icBR);

    var icTLjump = num2let(parseInt(let2num(c1))-2)+(parseInt(c2)-2).toString();
    var icTRjump = num2let(parseInt(let2num(c1))-2)+(parseInt(c2)+2).toString();
    var icBLjump = num2let(parseInt(let2num(c1))+2)+(parseInt(c2)-2).toString();
    var icBRjump = num2let(parseInt(let2num(c1))+2)+(parseInt(c2)+2).toString();
    
    var sTLjump = squareStatus(icTLjump),sTRjump = squareStatus(icTRjump);
    var sBLjump = squareStatus(icBLjump),sBRjump = squareStatus(icBRjump);

    var aTL,aTR,aBL,aBR,aTLj,aTRj,aBLj,aBRj;

    if (sSelf == 'red') {
        if (sTL == 'red') {aTL = false,aTLj = false;} else if (sTL == 'black') {aTL = false;
            if (sTLjump == 'empty') {aTLj = true;} else {aTLj = false;}
        } else if (sTL == 'empty') {aTL = true;aTLj = false;}

        if (sTR == 'red') {aTR = false,aTRj = false;} else if (sTR == 'black') {aTR = false;
            if (sTRjump == 'empty') {aTRj = true;} else {aTRj = false;}
        } else if (sTR == 'empty') {aTR = true,aTRj = false;}

        if (hasKing(inCoordinate)) {
            if (sBL == 'red') {aBL = false,aBLj = false;} else if (sBL == 'black') {aBL = false;
                if (sBLjump == 'empty') {aBLj = true;} else {aBLj = false;}
            } else if (sBL == 'empty') {aBL = true,aBLj = false;}

            if (sBR == 'red') {aBR = false,aBRj = false;} else if (sBR == 'black') {aBR = false;
                if (sBRjump == 'empty') {aBRj = true;} else {aBRj = false;}
            } else if (sBR == 'empty') {aBR = true,aBRj = false;}
        } else {aBL = false,aBLj = false,aBR = false,aBRj = false;}
        
    } else if (sSelf == 'black') {
        if (sBL == 'black') {aBL = false,aBLj = false;} else if (sBL == 'red') {aBL = false;
            if (sBLjump == 'empty') {aBLj = true;} else {aBLj = false;}
        } else if (sBL == 'empty') {aBL = true,aBLj = false;}

        if (sBR == 'black') {aBR = false,aBRj = false;} else if (sBR == 'red') {aBR = false;
            if (sBRjump == 'empty') {aBRj = true;} else {aBRj = false;}
        } else if (sBR == 'empty') {aBR = true,aBRj = false;}

        if (hasKing(inCoordinate)) {
            if (sTL == 'black') {aTL = false,aTLj = false;} else if (sTL == 'red') {aTL = false;
                if (sTLjump == 'empty') {aTLj = true;} else {aTLj = false;}
            } else if (sTL == 'empty') {aTL = true,aTLj = false;}

            if (sTR == 'black') {aTR = false,aTRj = false;} else if (sTR == 'red') {aTR = false;
                if (sTRjump == 'empty') {aTRj = true;} else {aTRj = false;}
            } else if (sTR == 'empty') {aTR = true,aTRj = false;}
        } else {aTL = false,aTLj = false,aTR = false,aTRj = false;}
    }
    
    return {
        "tl":{
            "status":aTL,
            "coordinate":icTL,
            "jump":{
                "status":aTLj,
                "coordinate":icTLjump.substr(0,1) + icTLjump.substr(1,1)
            }
        },
        "tr":{
            "status":aTR,
            "coordinate":icTR,
            "jump":{
                "status":aTRj,
                "coordinate":icTRjump.substr(0,1) + icTRjump.substr(1,1)
            }
        },
        "bl":{
            "status":aBL,
            "coordinate":icBL,
            "jump":{
                "status":aBLj,
                "coordinate":icBLjump.substr(0,1) + icBLjump.substr(1,1)
            }
        },
        "br":{
            "status":aBR,
            "coordinate":icBR,
            "jump":{
                "status":aBRj,
                "coordinate":icBRjump.substr(0,1) + icBRjump.substr(1,1)
            }
        }        
    };
  }

  function acceptableMoves(inMovesJSON) {
    var acceptArr = [];
    
    if (inMovesJSON['tl']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['tl']['coordinate']))}
    if (inMovesJSON['tr']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['tr']['coordinate']))}
    if (inMovesJSON['bl']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['bl']['coordinate']))}
    if (inMovesJSON['br']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['br']['coordinate']))}
    
    if (inMovesJSON['tl']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['tl']['jump']['coordinate']))}
    if (inMovesJSON['tr']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['tr']['jump']['coordinate']))}
    if (inMovesJSON['bl']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['bl']['jump']['coordinate']))}
    if (inMovesJSON['br']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['br']['jump']['coordinate']))}

    if (acceptArr.length == 0) {
        if (inMovesJSON['tl']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['tl']['coordinate'],1,1));
        } else if (inMovesJSON['tr']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['tr']['coordinate'],1,-1));
        } else if (inMovesJSON['bl']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['bl']['coordinate'],-1,1));
        } else if (inMovesJSON['br']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['br']['coordinate'],-1,-1));
        }
        
    } else if (acceptArr.length > 0) {
        return acceptArr.join();
    }
    
  }

  function coordinateMath(inCoordinate, letterShift, numberShift) {
    var c1 = num2let(parseInt(let2num(inCoordinate.substr(0,1)))+letterShift);
    var c2 = (parseInt(inCoordinate.substr(1,1))+numberShift).toString();
    return c1+c2;
  }

  function coordinateDifference(inCoordinate1, inCoordinate2) {
    
    var c11 = parseInt(let2num(c1c2(inCoordinate1)[0]));
    var c12 = parseInt(c1c2(inCoordinate1)[1]);
    var c21 = parseInt(let2num(c1c2(inCoordinate2)[0]));
    var c22 = parseInt(c1c2(inCoordinate2)[1]);
    return [c21-c11,c22-c12];
  }

  function coordinateDirection(sCoord, eCoord) {
    var cs1 = parseInt(let2num(c1c2(sCoord)[0]));
    var cs2 = parseInt(c1c2(sCoord)[1]);
    var ce1 = parseInt(let2num(c1c2(eCoord)[0]));
    var ce2 = parseInt(c1c2(eCoord)[1]);
    var verticalDirection;
    var horizontalDirection;

    if (cs1 > ce1) {
        verticalDirection = 't';
    } else if (cs1 < ce1) {
        verticalDirection = 'b';
    }

    if (cs2 > ce2) {
        horizontalDirection = 'l';
    } else if (cs2 < ce2) {
        horizontalDirection = 'r';
    }

    return verticalDirection+horizontalDirection;
  }

  function jumpSquares(inMovesJSON) {
    var acceptArr = [];
    
    if (inMovesJSON['tl']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['tl']['jump']['coordinate']))}
    if (inMovesJSON['tr']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['tr']['jump']['coordinate']))}
    if (inMovesJSON['bl']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['bl']['jump']['coordinate']))}
    if (inMovesJSON['br']['jump']['status'] == true) {acceptArr.push(coordinate2id(inMovesJSON['br']['jump']['coordinate']))}

    if (acceptArr.length == 0) {
        if (inMovesJSON['tl']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['tl']['jump']['coordinate'],2,2));
        } else if (inMovesJSON['tr']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['tr']['jump']['coordinate'],2,-2));
        } else if (inMovesJSON['bl']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['bl']['jump']['coordinate'],-2,2));
        } else if (inMovesJSON['br']['coordinate'] != undefined) {
            return coordinate2id(coordinateMath(inMovesJSON['br']['jump']['coordinate'],-2,-2));
        }
        
    } else if (acceptArr.length > 0) {
        return acceptArr.join();
    }
  }

  function becomeKing(inCoordinate) {
    var inID = _2ID(inCoordinate);
    var rowData = $(inID).parent().data('row');
    var piececolorData = $(inID+' div').data('piececolor');
    var endrowData = $(inID).parent().data('endrow');
    var kingData = $(inID+' div').data('king');
    

    if (endrowData == true) {
        if (kingData == false) {
            if (((piececolorData == 'red') && (rowData == 'black-end-row')) || ((piececolorData == 'black') && (rowData == 'red-end-row'))) {
                $(inID+' div').attr('data-king', true);
                $(inID+' div p').replaceWith('<p class="kingme">K</p>');
            }
        }
    }
  }



  $(function() {

    var dragPiece;
    var startSquare;
    var movesJSON;
    var destSquare;
    var endSquare = [null,null];
    var stopSquare;
    var droppableSquares;

    $(document).on('mouseenter', '.checker-piece', function() {
        movesJSON = possibleMoves(pullCoordinate($(this).parent().attr('id')));
        // console.log(movesJSON);
        var droppableSquares = acceptableMoves(movesJSON);
        // console.log(droppableSquares);

        $('.checker-piece').draggable({
          revert: "invalid",
          start: function(event, ui) {
            dragPiece = $(this)[0];
            startSquare = $(this).parent().attr('id');
          }
        });


        $(droppableSquares).droppable({
            accept: '.checker-piece',
            drop: function(event, ui) {
                var uiT = Math.round(ui['position']['top']/100);
                var uiL = Math.round(ui['position']['left']/100)
                stopSquare = coordinateMath(pullCoordinate(startSquare), uiT, uiL);
                destSquare = $(this).attr('id');
                var destSquareID = '#'+destSquare;
                var startSquareID = '#'+startSquare;
                endSquare = relativeMovement(pullCoordinate(startSquare), stopSquare);
                $(startSquareID+' div').replaceWith('<div class="checker-square-child" data-piececolor="empty">&nbsp;</div>');
                $(destSquareID+' div').replaceWith(dragPiece);
                $(destSquareID+' div').css('top','-1px');
                $(destSquareID+' div').css('left','-1px');
                if (jumpSquares(movesJSON).includes(destSquareID)) {
                    var startC = pullCoordinate(startSquare);
                    var destC = pullCoordinate(destSquare);
                    var jumpedID = coordinate2id(movesJSON[coordinateDirection(startC,destC)]['coordinate']);
                    $(jumpedID+' div').replaceWith('<div class="checker-square-child" data-piececolor="empty">&nbsp;</div>');
                }
                becomeKing(destSquareID);
            }
        });
    });
    

    
    

  });

