        } else if (/sq-[A-H]-[1-8]/.test(inString)) {
            return 'id';
        } else {
            return 'coordinate';
        }
    }

  function c1c2(inCoordinate) {return [inCoordinate.substr(0,1), inCoordinate.substr(1,1)];}

  function pullCoordinate(inID) {return inID.substr(3,1) + inID.substr(5,1);}

  function coordinate2id(inCoordinate) {
    var c1 = c1c2(inCoordinate)[0],c2 = c1c2(inCoordinate)[1];
    return '#sq-'+c1+'-'+c2;
  }

