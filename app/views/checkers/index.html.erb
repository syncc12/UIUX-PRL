  function relativeMovement(inSCoord, inECoord) {
    var sC1 = c1c2(inSCoord)[0],sC2 = c1c2(inSCoord)[1];

    var rmTL = num2let(parseInt(let2num(sC1))-1)+(parseInt(sC2)-1).toString();
    var rmTR = num2let(parseInt(let2num(sC1))-1)+(parseInt(sC2)+1).toString();
    var rmBL = num2let(parseInt(let2num(sC1))+1)+(parseInt(sC2)-1).toString();
    var rmBR = num2let(parseInt(let2num(sC1))+1)+(parseInt(sC2)+1).toString();

    var rmTLj = num2let(parseInt(let2num(sC1))-2)+(parseInt(sC2)-2).toString();
    var rmTRj = num2let(parseInt(let2num(sC1))-2)+(parseInt(sC2)+2).toString();
    var rmBLj = num2let(parseInt(let2num(sC1))+2)+(parseInt(sC2)-2).toString();
    var rmBRj = num2let(parseInt(let2num(sC1))+2)+(parseInt(sC2)+2).toString();

    var relJSON = '{"'+rmTL+'":["tl",null],"'+rmTR+'":["tr",null],"'+rmBL+'":["bl",null],"'+rmBR+'":["br",null],"'+rmTLj+'":["tl","jump"],"'+rmTRj+'":["tr","jump"],"'+rmBLj+'":["bl","jump"],"'+rmBRj+'":["br","jump"]}';

    relJSON = JSON.parse(relJSON);

    return relJSON[inECoord];
  }

