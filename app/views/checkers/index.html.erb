  function num2let(inNum) {return n2lDict[inNum.toString()];}

  function let2num(inLet) {return l2nDict[inLet.toString()];}

  function squareStatus(inCoordinate) {
    var c1 = c1c2(inCoordinate)[0],c2 = c1c2(inCoordinate)[1];
